@model JsReport.Models.InvoiceModel
<style>
    .heading > td {
        background-color: green;
        color: white;
    }
</style>
@{
    ViewData["Title"] = "Reporte";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<table>
    <tr class="heading ">
        <td>
            Item
        </td>
        <td>
            Price
        </td>
    </tr>

    @foreach (var item in Model.Items)
    {
<tr class="item">
    <td>
        @item.Name
    </td>
    <td>
        @item.Price
    </td>
</tr>}
</table>
